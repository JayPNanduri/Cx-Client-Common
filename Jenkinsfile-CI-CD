pipeline {
  parameters {        
        booleanParam(name: 'IsReleaseBuild', description: 'Check the box if you want to create a release build!') 
    }
  agent {
    node {
      label 'Plugins'
    }
  }
  tools {
        jdk 'JDK_WINDOWS_1.8.0_92'
  }
	
  stages {
     stage('Build') {
            steps {
		    echo "Build"
            }
        }
		stage('Archive Artifacts') {
			steps {
				echo "Archive Artifacts"
			}
		}  
  }
}
